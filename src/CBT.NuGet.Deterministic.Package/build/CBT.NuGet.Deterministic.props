<?xml version="1.0" encoding="utf-8"?>
<!--
 Lab builds would set ImportNuGetDeterministicProps to true.  Because NuGet re-evalutes the msbuild project with out considering global properties this should be set as an env var since this import must be in the graph on the nuget evaluation of the project.
 Consider cbt.nuget module to provide a kvp property that will be used in the nuget cmd invocation process.
 Because nuget doesn't import nuget package imports during restore this import line below must be placed in your root build props and the import must come before any <PackageReference elements are defined in your project.
 We only need to have the props imported durring nuget evaluation so using ExcludeRestorePackageImports == true will work modify this condition to add a property that is defined in your lab scenario only to have this import only occur in your lab builds during nuget evaluation.
 <Import Project="$(MSBuildProjectDirectory)\NuGetAssetsLock.props" Condition=" '$(ExcludeRestorePackageImports)' == 'true' " />
-->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <EnableCBTNuGetDeterministic Condition=" '$(EnableCBTNuGetDeterministic)' == '' ">true</EnableCBTNuGetDeterministic>
    <CBTNuGetDeterministicTasksAssemblyPath Condition=" '$(CBTNuGetDeterministicTasksAssemblyPath)' == '' ">$(MSBuildThisFileDirectory)\tasks\CBT.NuGet.Tasks.Deterministic.dll</CBTNuGetDeterministicTasksAssemblyPath>
    <CBTNuGetDeterministicGeneratedOutputPropsFile Condition=" '$(CBTNuGetDeterministicGeneratedOutputPropsFile)' == '' ">$(MSBuildProjectDirectory)\NuGetAssetsLock.props</CBTNuGetDeterministicGeneratedOutputPropsFile>
    <ImportNuGetDeterministicProps Condition=" '$(ImportNuGetDeterministicProps)' == '' ">false</ImportNuGetDeterministicProps>
    <CBTNuGetDeterministicOverWritePropsFile Condition=" '$(CBTNuGetDeterministicOverWritePropsFile)' == '' ">false</CBTNuGetDeterministicOverWritePropsFile>
    <GenerateNuGetDeterministicProps Condition=" '$(GenerateNuGetDeterministicProps)' == '' And '$(ImportNuGetDeterministicProps)' != 'true' ">true</GenerateNuGetDeterministicProps>
  </PropertyGroup>
</Project>
