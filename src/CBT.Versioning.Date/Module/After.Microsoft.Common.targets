<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <EnableVersioningDate Condition="'$(EnableVersioningDate)' != ''">$([System.Convert]::ToBoolean($(EnableVersioningDate)))</EnableVersioningDate>
    <EnableVersioningDate Condition="'$(EnableVersioningDate)' == ''">true</EnableVersioningDate>
    <EnableVersioningDateClickOnce Condition="'$(EnableVersioningDateClickOnce)' != ''">$([System.Convert]::ToBoolean($(EnableVersioningDateClickOnce)))</EnableVersioningDateClickOnce>
    <EnableVersioningDateClickOnce Condition="'$(EnableVersioningDateClickOnce)' == ''">true</EnableVersioningDateClickOnce>
    <EnableAutoImportAssemblyVersion Condition="'$(EnableAutoImportAssemblyVersion)' != ''">$([System.Convert]::ToBoolean($(EnableAutoImportAssemblyVersion)))</EnableAutoImportAssemblyVersion>
    <EnableAutoImportAssemblyVersion Condition="'$(EnableAutoImportAssemblyVersion)' == ''">true</EnableAutoImportAssemblyVersion>
  </PropertyGroup>

  <Import Project="Version.targets" Condition="'$(EnableVersioningDate)'=='true'"/>
  <Import Project="ClickOnce.Version.targets" Condition="'$(EnableVersioningDateClickOnce)'=='true'"/>

  <PropertyGroup>
    <AssemblyInfoGenerateFileDependsOn Condition="'$(VersionGenerateFileDependsOn)' != ''">$(AssemblyInfoGenerateFileDependsOn);$(VersionGenerateFileDependsOn)</AssemblyInfoGenerateFileDependsOn>
  </PropertyGroup>

</Project>