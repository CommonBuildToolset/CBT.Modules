<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
    <AssemblyVersionTemplateFileName Condition="'$(AssemblyVersionTemplateFileName)' == ''">AssemblyVersion.vb</AssemblyVersionTemplateFileName>
  </PropertyGroup>

  <ItemGroup Condition="'$(EnableAutoImportAssemblyVersion)' == 'true' And Exists('$(VersionTemplateDirectory)\$(AssemblyVersionTemplateFileName)')" >
    <Compile Include="$(VersionOutputPath)\$(AssemblyVersionTemplateFileName)" >
      <ExcludeFromSourceAnalysis>true</ExcludeFromSourceAnalysis>
      <Link>$(AssemblyInfoTemplateFileName)</Link>
    </Compile>
  </ItemGroup>

</Project>